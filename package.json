{
  "name": "jsenvelop",
  "version": "0.1.0",
  "description": "A personal project aimed at bundling javascript just like webpack, browserify, etc.",
  "browser": "es6-browser.js",
  "main": "es6.js",
  "scripts": {},
  "author": "phil olson",
  "license": "WTFPL",
  "devDependencies": {
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "deep-eql": "^2.0.2",
    "eslint": "^3.19.0",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "deep-eql": "^2.0.1",
    "delay": "^2.0.0",
    "lodash.curry": "^4.1.1",
    "lodash.kebabcase": "4.1.1",
    "pify": "^2.3.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": 0,
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "all",
          "ignoreRestSiblings": false,
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
